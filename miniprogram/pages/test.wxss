/* 答题页背景 */
.card-bg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: url(data:image/png;base64,) no-repeat;
  background-size:100% 100%;
}
/*https://i.loli.net/2020/04/06/PtkhlX9ZRpuoUiY.jpg*/
.card{
  width: 80%;
  height: 75%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  padding: 50rpx;
  box-sizing: border-box;
  background: linear-gradient(	#FFB6C1, #F08080);
  border-radius: 20px;
  color: #DAE1DA;
  opacity: 0.7;  
  filter: alpha(opacity=70);
  font-weight: 1000;
  font-size: 80px;
} 
.card::before{
  content: '';
  position: absolute;
  z-index: 6;
  top: 0;
  left: 0;
  transform: translate(-6%,-66%);
  width: 586rpx;
  height: 295rpx;
  background: url;/*(https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/card-pic-top.png?sign=75664d2884e507a0865525d96b43ab22&t=1582128084) no-repeat;*/
  background-size: 100% 100%;
}
.card::after{
  content: '';
  position: absolute;
  z-index: 6;
  bottom: 0;
  left: 0;
  transform: translate(6%,60%);
  width: 586rpx;
  height: 111rpx;
  background: url;/*(https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/card-pic-bottom.png?sign=690e74983bca70d4da4f978c106993b2&t=1582128109) no-repeat;*/
  background-size: 100% 100%;
}
.card-bg-2{
  background: url(https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/card-bg-2.png?sign=065c56adb00d0a9353d7645a18675682&t=1582132060) no-repeat;
  background-size: 100% 100%;
}
.card-bg-3{
  background: url(https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/card-bg-3.png?sign=10cbebde3d74958635e9d54631bc7181&t=1582132127) no-repeat;
  background-size: 100% 100%;
}
.card-bg-4{
  background: url(https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/card-bg-4.png?sign=b202aef9a2f2287a6210b6b7155d44ea&t=1582132138) no-repeat;
  background-size: 100% 100%;
}
.card-bg-5{
  background: url(https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/card-bg-5.png?sign=daf61e74893357fe3bea6b3d6477ceeb&t=1582132181) no-repeat;
  background-size: 100% 100%;
}
/* logo */
.logo{
  position: absolute;
  top: 5%;
  left: 8%;
  width: 93rpx;
}
/* 首页标题 */
.cover1-tit{
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translate(-44.6%,-50%);
  width: 507rpx;
}
/* 首页展示图 */
.cover1-pic{
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%,0%);
  width: 574rpx;
}
/* 首页按钮 */
.start-btn{
  position: absolute;
  top: 66%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 46rpx;
  width: 50vw!important;
  color: #fff;
  background-color: #1f8877;
}
.start-btn:active{
  background-color: #116355;
}
.finnal{
  position: absolute;
  top: 63%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 46rpx;
  /* width: 50vw; */
  background:#1f8877;
  color:#fff;
}


.question{
  padding-left: 1rpx;
  width: 100%;
  text-align: left;
  font-size: 36rpx;
  border-radius: 30px;
  color:#312354;
  margin-bottom: 100rpx;
}
/* 问题选项 */
.card .choice{
  width: 100%;
  /*overflow: hidden;*/
  margin-bottom: 50rpx;
  margin-top: -40px;
}
.card .choice button{
  padding-left: 20rpx;
  display: block;
  height: 100rpx!important;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  line-height: 75rpx;
  border-radius: 10px;
  background: rgb(235, 243, 235);
  color:#312354;
  background-color: #fff;
  letter-spacing:2rpx;
  width: 100%!important;
  text-align: left;
}
.card .choice button.cur{
  background-color: #43a192;
  color: #fff;
}
/* 选项提示 */
.card .tips{
  width: 100%;
  border-radius: 10rpx;
  padding: 10rpx;
  font-size: 28rpx;
  color: #fff;
  position:relative;
  top:-20px;
  opacity: 1;  
}
.card .tips1{
  background: linear-gradient(#1DE5E2, #B588F7);
    opacity: 1;  
}
.card .tips2{
  background: linear-gradient(#FF7F50, #FFD700);
    opacity: 1;  
}
.card .tips::after{
  content: ' 点击继续答题';
  color: #90EE90;
}