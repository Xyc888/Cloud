/* miniprogram/pages/index.wxss */

/* 首页背景 */
.cover1{
  width: 100vw;
  height: 100vh;
  background: url(https://ss1.bdstatic.com/70cFuXSh_Q1YnxGkpoWK1HF6hhy/it/u=395456624,3542168775&fm=26&gp=0.jpg) no-repeat;
  background-size:100% 100%;
}
.cover1-bg{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 90%;
  height: 90%;
  background: url(data:image/png;base64,) no-repeat;
  background-size:100% 100%;
}

/*https://i.loli.net/2020/04/06/kKfo1CWZ69sNjvn.png*/
/*https://6d61-mask-55bvo-1301214208.tcb.qcloud.la/cover-inner.png?sign=da2c863729afb88fbb73081e42b1a0cd&t=1582123191*/

/* logo */
.logo{
  position: absolute;
  top: 5%;
  left: 8%;
  width: 93rpx;
}
/* 首页标题 */
.cover1-tit{
  position: absolute;
  top: 27%;
  left: 47%;
  transform: translate(-44.6%,-50%);
  width: 507rpx;
  height: 700rpx;
}
/* 首页展示图 */
.cover1-pic{
  position: absolute;
  bottom: 30px;
  left: 50%;
  transform: translate(-50%,0%);
  width: 574rpx;
}
/* 首页按钮 */
.start-btn{
  position: absolute;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 46rpx;
  width: 50vw!important;
  color: #fff;
  background-color: #FFDEAD;
}


.start-btn:active{
    background-color: #F08080;
}
.start-btn.auth{
  background-color: transparent;
  color: #666;
  font-size: 14px;
  top: 55%;
}